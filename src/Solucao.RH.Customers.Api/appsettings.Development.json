{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Apis": {
    "SnapTrace": {
      "BaseAddress": "http://localhost:5006",
      "EndPoints": [
        {
          "post-log": "xxxx/xxxx"
        }
      ]
    },
    "Customer-hist": {
      "BaseAddress": "https://localhost:9001",
      "EndPoints": [
        {
          "customer-hist": "api/v1/hist-customer",
          "contact-hist": "api/v1/hist-contact",
          "address-hist": "api/v1/hist-address"
        }
      ]
    }
  }
}
